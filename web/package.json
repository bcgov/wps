  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "5.15.20",
    "@mui/x-data-grid-pro": "^6.0.0",
    "@mui/x-date-pickers": "^7.0.0",
    "@psu/cffdrs_ts": "^0.1.0",
    "@reduxjs/toolkit": "^1.8.0",
    "@sentry/react": "^8.2.1",
    "@types/esri-leaflet": "^3.0.0",
    "@types/leaflet": "^1.7.0",
    "@types/lodash": "^4.14.173",
    "@types/luxon": "^3.0.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-plotly.js": "^2.2.4",
    "@types/react-router-dom": "^5.3.0",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-istanbul": "^2.0.5",
    "axios": "1.7.4",
    "date-fns": "^3.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.4",
    "esri-leaflet": "3.0.12",
    "filefy": "^0.1.11",
    "jsdom": "^24.1.1",
    "jwt-decode": "^4.0.0",
    "keycloak-js": "^25.0.0",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.21",
    "luxon": "^3.0.2",
    "match-sorter": "^6.3.1",
    "nyc": "^17.0.0",
    "ol": "10.0.0",
    "ol-pmtiles": "^0.5.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^8.0.0",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.1.8",
    "typescript": "^5.2.2",
    "vitest": "^2.0.5"
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "dev:cypress": "export $(cat .env.cypress | xargs) && yarn dev",
    "build:prod": "tsc -b && vite build",
    "test": "vitest",
    "test:ci": "CI=true npm test",
    "coverage": "yarn test -- --coverage --watchAll=false",
    "coverage:ci": "CI=true VITE_KEYCLOAK_CLIENT=wps-web vitest run --coverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome --config watchForFileChanges=false",
    "cypress:and:server": "start-server-and-test start:cypress 3030 cy:open",
    "cypress:ci": "start-server-and-test dev:cypress http://localhost:3030 'cy:run'",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md)\"",
    "finalizeCoverage": "yarn node mergeCoverage.cjs"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/instrument-cra": "^1.4.0",
    "@eslint/compat": "^1.1.1",
    "@sentry/vite-plugin": "^2.22.2",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/recharts": "^1.8.23",
    "@vitejs/plugin-react": "^4.3.1",
    "cypress": "^13.13.3",
    "eslint": "^9.7.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "globals": "^15.8.0",
    "start-server-and-test": "^2.0.0",
    "ts-sinon": "^2.0.2",
    "vite": "^5.3.4",
    "vite-plugin-istanbul": "^6.0.2",
    "vite-plugin-svgr": "^4.2.0"
  },
  "nyc": {
    "report-dir": "coverage-cypress",
    "excludeAfterRemap": true,
    "exclude": [
      "src/serviceWorker.ts",
      "src/app/store.ts"
    ]
  },
  "packageManager": "yarn@4.3.1"
}
