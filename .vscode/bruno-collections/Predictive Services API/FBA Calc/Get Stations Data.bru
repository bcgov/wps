meta {
  name: Get Stations Data
  type: http
  seq: 1
}

post {
  url: {{API_BASE_URL}}/fba-calc/stations
  body: json
  auth: bearer
}

auth:bearer {
  token: {{AUTH_TOKEN}}
}

body:json {
  {
    "date": "",
    "stations": [
