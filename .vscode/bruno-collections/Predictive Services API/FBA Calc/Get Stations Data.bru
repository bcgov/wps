        "fuel_type": "",
        "percentage_conifer": "",
        "percentage_dead_balsam_fir": "",
        "grass_cure": "",
        "crown_base_height": "",
        "crown_fuel_load": "",
        "wind_speed": "",
        "precipitation": ""
      }
    ]
  }
}
